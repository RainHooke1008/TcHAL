    <Method Name="register_channel" Id="{b7d579a0-2316-0701-2150-59916b5f47b8}" FolderPath="Init\">
      <Declaration><![CDATA[METHOD register_channel : BOOL
VAR_INPUT
    terminalId : UINT;
    I_interface_manager : I_interfaceManager;
END_VAR
VAR_OUTPUT
	errId		: HAL_ErrorEnum;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.register_I_AI_percentage(terminalId:=terminalId , I_interface_manager:=I_interface_manager , errId=>errId );
IF errId = HAL_ErrorEnum.no_error THEN
	THIS^.register_I_AI_Temperature(terminalId:=terminalId , I_interface_manager:=I_interface_manager , errId=>errId );
END_IF
]]></ST>
      </Implementation>
    </Method>
