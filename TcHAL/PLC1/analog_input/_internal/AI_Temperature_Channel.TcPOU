    <Property Name="rValue" Id="{0ef95a23-2848-052e-1f94-5118a5eab3bc}" FolderPath="HAL\">
      <Declaration><![CDATA[PROPERTY rValue : REAL
]]></Declaration>
      <Get Name="Get" Id="{6586983f-bec8-0b4e-0f34-dfafb3f79d78}">
        <Declaration><![CDATA[VAR    
END_VAR
]]></Declaration>
